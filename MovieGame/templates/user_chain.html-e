{% extends "base.html" %}

{% block content %}

<div class='game'>
 
   <div class='high-scores'> 
        <h1 class="goldName">{{ username[1] }}'s chain:</h1>



        <h2>Score: {{ score }} </h2>

        <div class='chain'>
            {% for item in chain %}
                {{ item[0] }}<br />
            {% endfor %}

        </div>

   </div>

</div>

{% endblock %}   