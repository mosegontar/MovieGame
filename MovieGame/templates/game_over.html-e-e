<!DOCTYPE html>
<link rel=stylesheet type=text/css href="{{ url_for('static', filename='style.css')}}">
<link href='https://fonts.googleapis.com/css?family=Slabo+27px|Poiret+One|Open+Sans:400,600,700' rel='stylesheet' type='text/css'><title>The Movie Game</title>

<div class='game'>
	<div>

		<h1 id='theEnd'> The End </h1>

		<form method="POST" action="/">
		    <h2 class="finalScore">{{ name }}'s score: {{ score }}</h2>

		    <input id="endSubmit" type="submit" name="restart" value='Play Again!' />
		</form>

		<form methods="POST" action="/high-scores">
			<input id="highScoreButton" type="submit" name="highScoreButton" value='High Scores' />
		</form>
		     
		<h3 id="endReel"> * * * * </h3>
		<div class='Reel'>
		    {% for item in chain %}
		        <li> {{ item }} </li>
		    {% endfor %}
		</div>
	</div>
</div>
